host = 'localhost'
port = 9090
uri = ''
framed = False
ssl = False
validate = True
ca_certs = None
keyfile = None
certfile = None
http = False
argi = 1

if sys.argv[argi] == '-h':
    parts = sys.argv[argi + 1].split(':')
    host = parts[0]
    if len(parts) > 1:
        port = int(parts[1])
    argi += 2

if sys.argv[argi] == '-u':
    url = urlparse(sys.argv[argi + 1])
    parts = url[1].split(':')
    host = parts[0]
    if len(parts) > 1:
        port = int(parts[1])
    else:
        port = 80
    uri = url[2]
    if url[4]:
        uri += '?%s' % url[4]
    http = True
    argi += 2

if sys.argv[argi] == '-f' or sys.argv[argi] == '-framed':
    framed = True
    argi += 1

if sys.argv[argi] == '-s' or sys.argv[argi] == '-ssl':
    ssl = True
    argi += 1

if sys.argv[argi] == '-novalidate':
    validate = False
    argi += 1

if sys.argv[argi] == '-ca_certs':
    ca_certs = sys.argv[argi+1]
    argi += 2

if sys.argv[argi] == '-keyfile':
    keyfile = sys.argv[argi+1]
    argi += 2

if sys.argv[argi] == '-certfile':
    certfile = sys.argv[argi+1]
    argi += 2

cmd = sys.argv[argi]
args = sys.argv[argi + 1:]

if http:
    transport = THttpClient.THttpClient(host, port, uri)
else:
    if ssl:
        socket = TSSLSocket.TSSLSocket(host, port, validate=validate, ca_certs=ca_certs, keyfile=keyfile, certfile=certfile)
    else:
        socket = TSocket.TSocket(host, port)
    if framed:
        transport = TTransport.TFramedTransport(socket)
    else:
        transport = TTransport.TBufferedTransport(socket)
protocol = TBinaryProtocol(transport)
client = QuantumNetworkService.Client(protocol)
transport.open()

if cmd == 'register_client':
    if len(args) != 1:
        print('register_client requires 1 args')
        sys.exit(1)
    pp.pprint(client.register_client(args[0],))

elif cmd == 'get_clear_qc':
    if len(args) != 1:
        print('get_clear_qc requires 1 args')
        sys.exit(1)
    pp.pprint(client.get_clear_qc(args[0],))

elif cmd == 'request_qubits':
    if len(args) != 2:
        print('request_qubits requires 2 args')
        sys.exit(1)
    pp.pprint(client.request_qubits(eval(args[0]), args[1],))

elif cmd == 'send_qubits':
    if len(args) != 3:
        print('send_qubits requires 3 args')
        sys.exit(1)
    pp.pprint(client.send_qubits(args[0], args[1], eval(args[2]),))

elif cmd == 'run':
    if len(args) != 2:
        print('run requires 2 args')
        sys.exit(1)
    pp.pprint(client.run(eval(args[0]), args[1],))

elif cmd == 'inbox':
    if len(args) != 1:
        print('inbox requires 1 args')
        sys.exit(1)
    pp.pprint(client.inbox(args[0],))

elif cmd == 'get_overall_qc':
    if len(args) != 0:
        print('get_overall_qc requires 0 args')
        sys.exit(1)
    pp.pprint(client.get_overall_qc())

elif cmd == 'reset_network_state':
    if len(args) != 0:
